programa
{
	
	funcao inicio()
	{
	   cadeia senha
    escreva("Digite uma senha: ")
    leia(senha)

    enquanto(senha == "12345" ou senha == "admin" ou senha == "senha"ou senha == "password"){
      escreva("Senha Fraca. Digite outra. \n")
      escreva("Digite uma senha: ")
      leia(senha)
      escreva("Senha segura!")
	}
}
}
